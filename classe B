class B{
    public int B1;
    public float B2;
    
    public B(){}
    
    public int getB1(){
        return B1;
    }
    
    public void setB1(int b1){
        B1 = b1;
    }
    
    public float getB2(){
        return B2;
    }
    
    public void setB2(int b2){
        B2 = b2;
    }
    
    public void MB1(){
        System.out.println("Metodo MB1");
    }
    
    public void MB2(){
        System.out.println("Metodo MB2");
    }
}
